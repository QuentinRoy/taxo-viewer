<table id="{{id}}">
    <thead>
    {{#each headerRows}}
        <tr>
        {{#each this}}
            <th colspan="{{width}}">
                {{node.category}}
                {{#if bottomBar}}<div class="hbar"></div>{{/if}}
            </th>
        {{/each}}
        </tr>
    {{/each}}
    </thead>
    <tbody>
    {{#each bodyRows}}
        <tr>
        {{#each this}}
            <td {{#if this}}data-bib-id="{{id}}"{{/if}}><span class="ref-entry">{{#if this}}{{#if url}}<a href={{url}}>{{/if ~}}
                {{authors/0/lastName}} {{biblio.entryTags.year}}
            {{#if url ~}}</a>{{/if}}{{/if}}</span></td>
        {{/each}}
        </tr>
    {{/each}}
    </tbody>
</table>