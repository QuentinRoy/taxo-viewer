<table id="{{id}}">
    <thead>
    {{#each headerRows}}
        <tr>
        {{#each this}}
            <th colspan="{{width}}">
                {{category}}
                {{#if this.hasMultipleSub}}<div class="hbar"></div>{{/if}}
            </th>
        {{/each}}
        </tr>
    {{/each}}
    </thead>
    <tbody>
    {{#each bodyRows}}
        <tr>
        {{#each this}}
            <td>{{#if this}}{{biblio.authors.0.lastName}} {{biblio.entryTags.year}}{{/if}}</th>
        {{/each}}
        </tr>       
    {{/each}}
    </tbody>
</table>